main
	add v0, zr, 15		; v0  Quantidade numeros
	add v1, zr, 0		; v1  contador
	add v2, zr, 0		; v2  começa em 0
	add v3, zr, 1		; v3  começa em 1
	add v4, zr, 0x40	; v4  salva a memoria
	add v6, zr, 1		; v6 incremento

loop_calculo
	bge v1, v0, fim_programa ; se v1>=v0, encerra o programa
	stw v2, v4		; armazena o valor
	add v5, v2, 0		; v5  copia o fib
	add v5, v3		; v5 proximo numero
	add v2, v3, 0		; atualiza e vai para o prox loop
	add v3, v5, 0		; atualiza e vai para o prox loop

	add v4, v4, 2		; avanca de 2 em 2
	add v1, v6		; incremento
	beq zr, zr, loop_calculo	;loop

fim_programa

	hlt
