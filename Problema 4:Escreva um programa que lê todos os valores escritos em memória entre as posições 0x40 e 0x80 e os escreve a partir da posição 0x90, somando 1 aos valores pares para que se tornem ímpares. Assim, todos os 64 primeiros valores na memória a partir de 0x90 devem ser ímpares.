main
	add v1, zr, array		; v1 array inicio
	add v2, zr, a_limit		; v2 final do array
	add v3, zr, 0x90		; v3 memoria 0x90
	add v4, zr, 1			; v4 recebe 1
	add v5, zr, 2			; v5 recebe 2

loop1
	bge v1, v2, endloop		; v1>=v2, termina
	ldw v6, v1, 0			; v6 valor atual do array
	add v7, v6, 0			; v7  copia do valor 
	and v7, v4			; v7 recebe v4
	bne v7, zr, eh_impar		; se (resultado != 0), o numero ja eh impar, pula
	add v6, v4			; se for parar, soma 1

eh_impar
	stw v6, v3, 0			;valor final
	add v1, v5			; avanca 2
	add v3, v5			; avanca 2
	beq zr, zr, loop1		; repete

endloop
	hlt

array				
	10 15 20 25 30 35 40 42
	50 55 58 59 65 70 72 80
	85 90 92 95 97 99 100 101
	102 105 107 110 115 118 119 120
	125 130 135 140 145 150 155 160
a_limit
